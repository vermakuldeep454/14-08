{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/light-mode",
      "dest": "/light-mode/index.html"
    },
    {
      "src": "/dark-mode",
      "dest": "/dark-mode/index.html"
    },
    {
      "src": "/default",
      "dest": "/default/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ]
}
